<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.DatePicker?>

<VBox styleClass="hbox-container" stylesheets="@../css/application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.CreateTicketController">
    <children>
      <VBox prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: beige;">
         <children>
             <Text text="Create a Ticket" />


             <ListView fx:id="projectList" />   
             <Label text="Name" />
             <TextField fx:id="name" promptText="Project Name" />
             <Label text="Description" />
             <TextArea fx:id="description" promptText="Description" />
			 <Button onAction="#createTicket" text="Create Ticket" />
         </children>
      </VBox>
    </children>
</VBox>
