<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox styleClass="hbox-container"  fx:controller="application.controller.MainController" stylesheets="@../css/application.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>

        <VBox prefHeight="720.0" prefWidth="1280.0" styleClass="vbox-container">
            <children>
                <Text styleClass="heading" text="Homepage" />
                <Hyperlink text="Create a New Project" onAction="#homeToNewProject"/>
                
                <Text styleClass="heading" text="Your Projects" />
                <ListView fx:id="projectList" />
            </children>
        </VBox>
    </children>
</HBox>
